    HostName da2.eecs.utk.edu
    User eponcemo
    Port 443 

# COSC 690 docker container on da2
# ssh -p 9155 -L8888:127.0.0.1:8888 eponcemo@da2.eecs.utk.edu
Host dda
    HostName da2.eecs.utk.edu
    User eponcemo
    Port 9155
    LocalForward 8888 127.0.0.1:8888

